{
  "version": 2,
  "builds": [
    { "src": "wp-config.php", "use": "@now/wordpress",
      "config": { "releaseUrl": "https://wordpress.org/wordpress-5.0.1.zip" } }
  ],
  "routes": [
    { "src": "/", "dest": "wp-config.php" },
    { "src": "/wp-admin", "dest": "wp-config.php" },
    { "src": "/wp-admin/", "dest": "wp-config.php" },
    { "src": "^\\\/.*\\.php", "dest": "wp-config.php" }
  ]
}
