{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@now/node" },
    { "src": "subdirectory/index.js", "use": "@now/node" }
  ],
  "probes": [
    { "path": "/", "mustContain": "root:RANDOMNESS_PLACEHOLDER" },
    { "path": "/subdirectory/", "mustContain": "subdir:RANDOMNESS_PLACEHOLDER" }
  ]
}
